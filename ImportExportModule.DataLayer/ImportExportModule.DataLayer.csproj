    <ItemGroup>
      <PackageReference Include="MongoDB.Driver" Version="2.21.0" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\ImportExportModule.Application\ImportExportModule.Application.csproj" />
    </ItemGroup>


</Project>
